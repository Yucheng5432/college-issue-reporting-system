<div class="private">
  <h1>Welcome, {{username}}</h1> 
   <h2>All Posts</h2>
    {{#if posts.length}}
    {{#each posts}}
    <div class="card">
        <h4>{{title}}</h4>
        <h5>{{username}}</h5>
        <p>{{body}}</p> 
        {{#if comments.length}}
        {{#each comments}}
        <p>{{userName}}</p>
        <p>{{body}}</p>
        {{/each}}
        {{/if}}
    </div>
    {{/each}}
    {{/if}}
  <a href="/logout">Logout</a>
</div>